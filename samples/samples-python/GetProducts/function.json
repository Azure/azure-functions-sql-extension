{
    "bindings": [
      {
        "authLevel": "function",
        "name": "req",
        "type": "httpTrigger",
        "direction": "in",
        "methods": [
          "get"
        ],
        "route": "getproducts/{cost}"
      },
      {
        "name": "$return",
        "type": "http",
        "direction": "out"
      },
      {
        "name": "products",
        "type": "sql",
        "direction": "in",
        "commandText": "select * from Products where Cost = @Cost",
        "commandType": "Text",
        "parameters": "@Cost={cost}",
        "connectionStringSetting": "SqlConnectionString"
      }
    ],
    "disabled": false
  }
{
  "scriptFile": "__init__.py",

  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous",
      "route": "getproducts-nameempty/{cost}"
    },
    {
      "direction": "in",
      "type": "sql",
      "name": "rowList",
      "commandText": "SELECT * FROM Products WHERE Cost = @Cost AND Name = @Name",
      "commandType": "Text",
      "parameters": "@Cost={cost},@Name=",
      "connectionStringSetting": "SqlConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}